{
  "name": "wiz_qiankun_demo",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "build": "vue-cli-service build --mode build",
    "lint": "vue-cli-service lint",
    "lint-fix": "eslint --fix --ext .js",
    "setup": "npm-run-all --serial setup:*",
    "setup:main": "yarn",
    "setup:plant": "cd children/vue-a && yarn",
    "setup:data": "cd children/vue-b && yarn",
    "start": "npm-run-all --parallel start:*",
    "start:vue-a": "cd children/vue-a && yarn serve",
    "start:vue-b": "cd children/vue-b && yarn serve",
    "start:main": "yarn serve",
    "deploy": "npm-run-all --parallel deploy:*",
    "deploy:main": "yarn build",
    "deploy:plant": "cd children/vue-a && yarn build",
    "deploy:data": "cd children/vue-b && yarn build",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/**.{js,json,pcss,md,vue}": [
      "node ./lint-prettier.js",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^2.6.5",
    "element-ui": "^2.15.3",
    "qiankun": "^2.4.3",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^4.7.1",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "lint-staged": "^8.1.5",
    "husky": "^1.3.1",
    "chalk": "^2.4.2",
    "fs": "^0.0.1-security"
  }
}
